
//풀이
//구현

function compareFunc(a, b) {
    return a - b;
}

function solution(numbers) {
    let answer = 0;
    let findNumber = 0;

    numbers.sort(compareFunc);

    numbers.forEach(num => {
        while(num !== findNumber) {
            answer += findNumber;
            findNumber++;
        }
        findNumber++;
    })

    const lastNumber = numbers[numbers.length - 1] + 1;
    for (let i = lastNumber; i <= 9; i++) answer += i;
    return answer;
}
